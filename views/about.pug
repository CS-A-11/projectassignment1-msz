    // =-=-=-=-=-=-= Light Header =-=-=-=-=-=-=
    .colored-header
      // Top Bar
      .header-top
        .container
          .row
            // Header Top Left
            .header-top-left.col-md-8.col-sm-6.col-xs-12.hidden-xs
              ul.listnone
                li
                  a(href="about.html")
                    i.fa.fa-heart-o(aria-hidden="true")
                    |  About
                |                         
                li
                  a(href="faqs.html")
                    i.fa.fa-folder-open-o(aria-hidden="true")
                    |  FAQS
            // Header Top Right Social
            .header-right.col-md-4.col-sm-6.col-xs-12
              .pull-right
                ul.listnone
                  li
                    a(href="login.html")
                      i.fa.fa-sign-in
                      |  Log in
                  |                            
                  li
                    a(href="register.html")
                      i.fa.fa-unlock(aria-hidden="true")
                      |  Register
      // Top Bar End
      // Navigation Menu
      nav#menu-1.mega-menu
        // menu list items container
        section.menu-list-items
          .container
            .row
              .col-lg-12.col-md-12
                // menu logo
                ul.menu-logo
                  li
                    a(href="index.html")
                      img(src="images/logo21.png", alt="HOSTLYTE")
                // menu links
                ul.menu-links
                  // active class
                  li
                    a(href="javascript:void(0)")
