doctype html
html(lang="en")
  head
    meta(http-equiv="content-type", content="text/html;charset=UTF-8")
    //if IE
      meta(http-equiv="X-UA-Compatible", content="IE=edge")
    |
    meta(name="description", content="")
    |
    meta(name="author", content="ScriptsBundle")
    |
    title Hostlyte| Largest Hostel Classified Portal
    // =-=-=-=-=-=-= Favicons Icon =-=-=-=-=-=-=
    link(rel="icon", href="images/favicon.ico", type="image/x-icon")
    // =-=-=-=-=-=-= Mobile Specific =-=-=-=-=-=-=
    meta(name="viewport", content="width=device-width, initial-scale=1, maximum-scale=1")
    // =-=-=-=-=-=-= Bootstrap CSS Style =-=-=-=-=-=-=
    link(rel="stylesheet", href="css/bootstrap.css")
    // =-=-=-=-=-=-= Template CSS Style =-=-=-=-=-=-=
    link(rel="stylesheet", href="css/style.css")
    // =-=-=-=-=-=-= Font Awesome =-=-=-=-=-=-=
    link(rel="stylesheet", href="css/font-awesome.css", type="text/css")
    // =-=-=-=-=-=-= Flat Icon =-=-=-=-=-=-=
    link(href="css/flaticon.css", rel="stylesheet")
    // =-=-=-=-=-=-= Et Line Fonts =-=-=-=-=-=-=
    link(rel="stylesheet", href="css/et-line-fonts.css", type="text/css")
    // =-=-=-=-=-=-= Menu Drop Down =-=-=-=-=-=-=
    link(rel="stylesheet", href="css/forest-menu.css", type="text/css")
    // =-=-=-=-=-=-= Animation =-=-=-=-=-=-=
    link(rel="stylesheet", href="css/animate.min.css", type="text/css")
    // =-=-=-=-=-=-= Select Options =-=-=-=-=-=-=
    link(href="css/select2.min.css", rel="stylesheet")
    // =-=-=-=-=-=-= noUiSlider =-=-=-=-=-=-=
    link(href="css/nouislider.min.css", rel="stylesheet")
    // =-=-=-=-=-=-= Listing Slider =-=-=-=-=-=-=
    link(href="css/slider.css", rel="stylesheet")
    // =-=-=-=-=-=-= Owl carousel =-=-=-=-=-=-=
    link(rel="stylesheet", type="text/css", href="css/owl.carousel.css")
    |
    link(rel="stylesheet", type="text/css", href="css/owl.theme.css")
    // =-=-=-=-=-=-= Check boxes =-=-=-=-=-=-=
    link(href="skins/minimal/minimal.css", rel="stylesheet")
    // =-=-=-=-=-=-= Responsive Media =-=-=-=-=-=-=
    link(href="css/responsive-media.css", rel="stylesheet")
    // =-=-=-=-=-=-= Template Color =-=-=-=-=-=-=
    link#color(rel="stylesheet", href="css/colors/defualt.css")
    // =-=-=-=-=-=-= For Style Switcher =-=-=-=-=-=-=
    link#theme-color(rel="stylesheet", type="text/css", href="#")
    // =-=-=-=-=-=-= Check boxes =-=-=-=-=-=-=
    link(href="skins/minimal/minimal.css", rel="stylesheet")
    // JavaScripts
    script(src="js/modernizr.js")
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js")
      |
      script(src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js")
  |
  body
    // =-=-=-=-=-=-= Preloader =-=-=-=-=-=-=
    #loader-wrapper
      #loader
      |
      .loader-section.section-left
      |
      .loader-section.section-right
    // =-=-=-=-=-=-= Light Header =-=-=-=-=-=-=
    .colored-header
      // Top Bar
      .header-top
        .container
          .row
            // Header Top Left
            .header-top-left.col-md-8.col-sm-6.col-xs-12.hidden-xs
              ul.listnone
                li
                  a(href="about.html")
                    i.fa.fa-heart-o(aria-hidden="true")
                    |  About
                |
                li
                  a(href="faqs.html")
                    i.fa.fa-folder-open-o(aria-hidden="true")
                    |  FAQS
            // Header Top Right Social
            .header-right.col-md-4.col-sm-6.col-xs-12
              .pull-right
                ul.listnone
                  li
                    a(href="login.html")
                      i.fa.fa-sign-in
                      |  Log in
                  |
                  li
                    a(href="register.html")
                      i.fa.fa-unlock(aria-hidden="true")
                      |  Register
      // Top Bar End
      // Navigation Menu
      nav#menu-1.mega-menu
        // menu list items container
        section.menu-list-items
          .container
            .row
              .col-lg-12.col-md-12
                // menu logo
                ul.menu-logo
                  li
                    a(href="index.html")
                      img(src="images/logo21.png", alt="HOSTLYTE")
                // menu links
                ul.menu-links
                  // active class
                  li
                    a(href="javascript:void(0)")
                      | Home
                      i
                  |
                  li
                    a(href="javascript:void(0)")
                      | Listing
                      i
                  |
                  li
                    a(href="javascript:void(0)")
                      | Categories
                      i
                  |
                  li
                    a(href="contact.html") Contact
                |
                ul.menu-search-bar
                  li
                    a.btn.btn-light(href=" ")
                      i.fa.fa-plus(aria-hidden="true")
                      |  Post Free Ad
    // Navigation Menu End
    // =-=-=-=-=-=-= Light Header End  =-=-=-=-=-=-=
    // =-=-=-=-=-=-= Transparent Breadcrumb =-=-=-=-=-=-=
    .page-header-area
      .container
        .row
          .col-lg-12.col-md-12.col-sm-12.col-xs-12
            .header-page
              h1 Support Center
    // Small Breadcrumb
    .small-breadcrumb
      .container
        .breadcrumb-link
          ul
            li
              a(href="index.html") Home
            |
            li
              a.active(href="#") FAQS
    // Small Breadcrumb
    // =-=-=-=-=-=-= Transparent Breadcrumb End =-=-=-=-=-=-=
    // =-=-=-=-=-=-= Main Content Area =-=-=-=-=-=-=
    .main-content-area.clearfix
      // =-=-=-=-=-=-= Latest Ads =-=-=-=-=-=-=
      section.section-padding.error-page.pattern-bg
        // Main Container
        .container
          // Row
          .row
            // Middle Content Area
            .col-md-8.col-xs-12.col-sm-12
              ul.accordion
                li
                  h3.accordion-title
                    a(href="#") Why I should use Hostlyte?
                  |
                  .accordion-content
                    p  BECAUSE WE ARE THE BEST IN THE WORLD.
            |
            .col-md-4.col-xs-12.col-sm-12
              // Sidebar Widgets
              .blog-sidebar
                // Categories
                .widget
                  .widget-heading
                    h4.panel-title
                      a Saftey Tips
                  |
                  .widget-content
                    p.lead
                      | Posting an ad on
                      a(href="#") Hostlyte
                      |  is free! However, all ads must follow our rules:
                    |
                    ol
                      li Make sure you post in the correct category.
                      |
                      li Do not post the same ad more than once or repost an ad within 48 hours.
                      |
                      li Do not upload pictures with watermarks.
                      |
                      li Do not post ads containing multiple items unless it's a package deal.
                      |
                      li Do not put your email or phone numbers in the title or description.
                      |
                      li Make sure you post in the correct category.
                      |
                      li Do not post the same ad more than once or repost an ad within 48 hours.
                      |
                      li Do not upload pictures with watermarks.
                // Latest News
              // Sidebar Widgets End
            // Middle Content Area  End
          // Row End
        // Main Container End
      // =-=-=-=-=-=-= Ads Archives End =-=-=-=-=-=-=
      // =-=-=-=-=-=-= FOOTER =-=-=-=-=-=-=
      // =-=-=-=-=-=-= FOOTER =-=-=-=-=-=-=
      .footer-top
        .container
          .row
            .col-md-3.col-sm-6.col-xs-12
              // Info Widget
              .widget
                .logo
                  img(alt="HOSTLYTE", src="images/logo12.png")
                |
                p DESCRIPTION DAALO
                |
                ul
                  li
                    img(src="images/appstore.png", alt="")
                  |
                  li
                    img(src="images/googleplay.png", alt="")
              // Info Widget Exit
            |
            .col-md-3.col-sm-6.col-xs-12
              // Follow Us
              .widget.socail-icons
                h5 Follow Us
                |
                ul
                  li
                    a.fb(href="")
                      i.fa.fa-facebook
                    span Facebook
                  |
                  li
                    a.twitter(href="")
                      i.fa.fa-twitter
                    span Twitter
                  |
                  li
                    a.linkedin(href="")
                      i.fa.fa-linkedin
                    span Linkedin
                  |
                  li
                    a.googleplus(href="")
                      i.fa.fa-google-plus
                    span Google+
              // Follow Us End
            |
            .col-md-6.col-sm-6.col-xs-12
              // Newslatter
              .widget.widget-newsletter
                h5 Singup for Weekly Newsletter
                |
                .fieldset
                  p
                    | We may send you information about related events, webinars, products and services which we believe.
                  |
                  form
                    input(value="Enter your email address", type="text")
                    |
                    input.submit-btn(name="submit", value="Submit", type="submit")
              // Newslatter
      // Copyrights
      .copyrights
        .container
          .copyright-content
            .row
              .col-lg-12.col-md-12.col-sm-12.col-xs-12
                p
                  | © 2018 Hostlyte All rights reserved. Design by
                  a(href="github.com/msz991", target="_blank")  MSZ
      // =-=-=-=-=-=-= FOOTER END =-=-=-=-=-=-=
    // Main Content Area End
    // Post Ad Sticky
    a.sticky-post-button.hidden-xs(href="#")
      span.sell-icons
        i.flaticon-transport-9
      |
      h4 SELL
    // Back To Top
    a.cd-top(href="#0") Top
    // =-=-=-=-=-=-= JQUERY =-=-=-=-=-=-=
    script(src="js/jquery.min.js")
    // Bootstrap Core Css
    script(src="js/bootstrap.min.js")
    // Jquery Easing
    script(src="js/easing.js")
    // Menu Hover
    script(src="js/forest-megamenu.js")
    // Jquery Appear Plugin
    script(src="js/jquery.appear.min.js")
    // Numbers Animation
    script(src="js/jquery.countTo.js")
    // Jquery Smooth Scroll
    script(src="js/jquery.smoothscroll.js")
    // Jquery Select Options
    script(src="js/select2.min.js")
    // noUiSlider
    script(src="js/nouislider.all.min.js")
    // Carousel Slider
    script(src="js/carousel.min.js")
    |
    script(src="js/slide.js")
    // Image Loaded
    script(src="js/imagesloaded.js")
    |
    script(src="js/isotope.min.js")
    // CheckBoxes
    script(src="js/icheck.min.js")
    // Jquery Migration
    script(src="js/jquery-migrate.min.js")
    // Sticky Bar
    script(src="js/theia-sticky-sidebar.js")
    // Style Switcher
    script(src="js/color-switcher.js")
    // Template Core JS
    script(src="js/custom.js")
